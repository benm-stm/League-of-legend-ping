{
  "manifest_version": 2,

  "name": "Leaque of legends PING Test",
  "description": "This extension will test your league of legends ping delay",
  "version": "1.0",

  "browser_action": {
   "default_icon": "img/lol-16.png",
   "default_popup": "popup.html"
  },
  
    "icons": {
    "16":  "img/lol-16.png",
    "32":  "img/lol-32.png",
    "48":  "img/lol-48.png",
    "128": "img/lol-128.png"
  },
  
   "web_accessible_resources": [
   "lib/popup.js",
   "img/lol-16.png",
   "img/lol-32.png",
   "img/lol-48.png",
   "img/lol-128.png",
   "popup.html"
  ]
}